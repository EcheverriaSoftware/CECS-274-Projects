public class QNode {
    //Instance variables for the QNode class.
    // key is where our Person object is stored.
    Person key;
    // next is a link to our next QNode
    QNode next;

    //One arguement constuctor for our QNode class.
    public QNode(Person key){
        this.key = key;
        this.next = null;
    }
}
